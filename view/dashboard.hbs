        document.addEventListener('DOMContentLoaded', () => {
            const lastActiveTab = localStorage.getItem('activeTab') || 'students';
            showSection(lastActiveTab);
        });

        sidebarToggle.addEventListener('click', () => {
            sidebar.classList.toggle('open');
        });

        sidebarButtons.forEach(button => {
            button.addEventListener('click', async () => {
                const sectionId = button.getAttribute('data-section');

                if (sectionId === 'staff') {
                    try {
                        const response = await fetch(`/v1/api/getUserRole?userId={{user._id}}`);
                        const data = await response.json();

                        if (!data.success || !data.isSuperAdmin) {
                            showToast('You don\'t have permission to access the Staff section', 'error');
                            return;
                        }
                    } catch (error) {
                        console.error('Error fetching user role:', error);
                        showToast('Something went wrong. Please try again.', 'error');
                        return;
                    }
                }

                showSection(sectionId);

                if (window.innerWidth < 1024) {
                    sidebar.classList.remove('open');
                }
            });
        });

        function showSection(sectionId) {
            sidebarButtons.forEach(btn => {
                if (btn.getAttribute('data-section') === sectionId) {
                    btn.classList.add('active');
                } else {
                    btn.classList.remove('active');
                }
            });

            contentSections.forEach(section => {
                if (section.id === sectionId) {
                    section.classList.add('active');
                } else {
                    section.classList.remove('active');
                }
            });

            localStorage.setItem('activeTab', sectionId);
        }

        function showCourseSelectionModal(actionType) {
            currentAction = actionType;
            courseSelectionModal.style.display = 'flex';
        }

        function showYearSelectionModal(course) {
            selectedCourse = course;

            if (currentAction === 'studentRegister') {
                navigateToPage(course, 1);
                return;
            }

            selectedCourseName.textContent = courseConfig[course].name;

            yearOptions.innerHTML = '';

            courseConfig[course].years.forEach(yearInfo => {
                const yearOption = document.createElement('div');
                yearOption.className = 'course-option';
                yearOption.setAttribute('data-year', yearInfo.year);

                yearOption.innerHTML = `
                    <div class="course-option-icon">
                        <i class="fas fa-calendar-alt"></i>
                    </div>
                    <h2 class="course-option-title">${yearInfo.label}</h2>
                `;

                yearOption.addEventListener('click', () => {
                    navigateToPage(course, yearInfo.year);
                });

                yearOptions.appendChild(yearOption);
            });

            courseSelectionModal.style.display = 'none';
            yearSelectionModal.style.display = 'flex';
        }

        function navigateToPage(course, year) {
             if (currentAction === 'studentRegister') {
                const path = `/v1/api/getStudentRegister?course=${courseConfig[course].name}`;
                window.location.href = path;
                return;
            }
            const path = courseConfig[course].paths[currentAction][year];
            if (path) {
                window.location.href = path;
            } else {
                alert('Path not configured for this course and year combination.');
            }
        }

