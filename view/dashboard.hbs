                try {
                    const response = await fetch('/v1/api/getUserRole?userId={{user._id}}');
                    const data = await response.json();

                    if (data.success && (data.isAdmin && course === data?.department || data.isSuperAdmin)) {
                        showYearSelectionModal(course);
                    } else {
                        showToast('You don\'t have permission to access this department', 'error');
                    }
                } catch (error) {
